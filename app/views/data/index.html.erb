<%= form_tag({ action: :import }, multipart: true, class: "mx-4 my-2 space-y-2") do %>
  <%= file_field_tag 'data' %>
  <button type=submit, class="py-2 px-4 rounded-lg bg-green-400">Submit</button>
<% end %>
<%= button_to "Export", "/export.csv", method: "get", class: "mx-4 my-2 py-2 px-4 rounded-lg bg-green-400" %>